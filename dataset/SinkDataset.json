{
	"name": "SinkDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorageLinkedService",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
				"nullValue": "\\N",
				"treatEmptyAsNull": true,
				"skipLineCount": 0,
				"firstRowAsHeader": false
			},
			"fileName": "@CONCAT('Incremental-', pipeline().RunId, '.txt'",
			"folderPath": "adfv1blob/IncrementalCopy"
		}
	}
}